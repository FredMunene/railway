{
  "id": null,
  "title": "FiatRails Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Mint Intents",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(fiatrails_mint_intents_total)",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 3 }
    },
    {
      "type": "stat",
      "title": "Callbacks Processed",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(fiatrails_callbacks_total)",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 3 }
    },
    {
      "type": "graph",
      "title": "Retry Attempts",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(fiatrails_retry_attempts_total[5m])",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 3, "w": 12, "h": 7 }
    },
    {
      "type": "graph",
      "title": "DLQ Depth",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "fiatrails_dlq_depth",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 10, "w": 12, "h": 7 }
    }
  ]
}
